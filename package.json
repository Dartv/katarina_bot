    "mz": "^2.7.0",
    "ramda": "^0.24.1",
    "request": "^2.83.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.1.0",
