{
  "name": "katarina-bot",
  "version": "0.0.1",
  "main": "src/index.js",
  "author": "Dartv",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "./node_modules/.bin/rimraf dist",
    "build": "tsc",
    "prebuild": "yarn clean",
    "start": "node dist/index.js",
    "start:watch": "lookenv --path=./env/lookenv.dev.js -- ./node_modules/.bin/tsc-watch --onSuccess \"node ./dist/index.js\"",
    "test": "./node_modules/.bin/jest",
    "eslint": "./node_modules/.bin/eslint",
    "lint": "./node_modules/.bin/eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:staged": "./node_modules/.bin/lint-staged",
    "deploy:setup": "pm2 deploy ecosystem.config.js production setup",
    "deploy:update": "pm2 deploy ecosystem.config.js production update",
    "deploy:revert": "pm2 deploy ecosystem.config.js production revert 1",
    "deploy:restart": "pm2 deploy ecosystem.config.js production exec \"pm2 restart all\"",
    "deploy": "pm2 deploy ecosystem.config.js production --force"
  },
  "lint-staged": {
    "*.ts": [
      "yarn eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged"
    }
  },
  "dependencies": {
    "@types/ramda": "^0.26.40",
    "@types/request-promise-native": "^1.0.17",
    "agenda": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "danbooru": "^3.1.0",
    "date-fns": "^2.8.1",
    "deepmerge": "^4.2.2",
    "discord.js": "^11.5.1",
    "dotenv": "^4.0.0",
    "ffmpeg-binaries": "^4.0.0",
    "ghastly": "^0.5.0-rc.0",
    "gm": "^1.23.0",
    "ioredis": "^4.17.0",
    "lib-r-math.js": "^1.0.86",
    "mongoose": "^5.7.5",
    "mz": "^2.7.0",
    "node-fetch": "^2.6.0",
    "p-queue": "^6.3.0",
    "puppeteer": "^2.0.0",
    "ramda": "^0.24.1",
    "random-int": "^1.0.0",
    "repatch": "^1.3.11",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.0",
    "signale": "^1.4.0",
    "simple-youtube-api": "^5.2.0",
    "snoowrap": "^1.20.1",
    "string-similarity": "^3.0.0",
    "valid-url": "^1.0.9",
    "ytdl-core": "^0.29.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.2",
    "@types/cheerio": "^0.22.14",
    "@types/ioredis": "^4.16.2",
    "@types/jest": "^24.0.23",
    "@types/mongoose": "^5.5.43",
    "@types/node": "^12.12.11",
    "@types/node-fetch": "^2.5.4",
    "@types/puppeteer": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^4.0.6",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "lookenv": "^1.0.1",
    "ts-jest": "^24.2.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.2"
  }
}
